	updateCheck="true"
	monitoring="autodetect" 
	dynamicConfig="true">

	<diskStore path="java.io.tmpdir" />
	
	<cache name="test" 
		maxBytesLocalHeap="100M"
		timeToLiveSeconds="1800">
	</cache>

</ehcache>